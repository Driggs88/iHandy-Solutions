
<% if (typeof requestJob !== 'undefined') { %>
  <section>
    <% requestJob.forEach(function(blah) { %>
      <h3><%= blah.name %></h3>
      <img src="<%= blah.pic_path %>" alt="<%= blah.pic_name %>" />
    <% })%>
  </section>
<% } %>


